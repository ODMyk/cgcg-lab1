{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "CGCG Lab 1",
  "version": "0.1.0",
  "identifier": "com.cgcglab1.app",
  "build": {
    "beforeDevCommand": "yarn dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "yarn build",
    "frontendDist": "../dist"
  },
  "app": {
    "windows": [
      {
        "title": "CGCG Lab 1",
        "width": 1600,
        "height": 900,
        "center": true,
        "resizable": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": ["icons/icon.png", "icons/icon.icns", "icons/icon.ico"]
  }
}
